#!/bin/bash
sshpass -p "uowsag" scp -r $HOME/documents/School/Data\ Structures\ and\ Algorithms/PA* gp053@cs.uco.edu:~/                   #   sync for Programming II
sshpass -p "uowsag" scp -r $HOME/documents/School/Data\ Structures\ and\ Algorithms\ Lab/* gp053@cs.uco.edu:~/lab/            #   sync for Programming II Lab
